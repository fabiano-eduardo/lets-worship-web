{
  "name": "lets-worship",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "generate-pwa-assets": "pwa-assets-generator",
    "check": "vite build && tsc --noEmit",
    "graphql:codegen": "graphql-codegen --config codegen.ts",
    "graphql:codegen:watch": "graphql-codegen --config codegen.ts --watch",
    "graphql:schema": "tsx scripts/introspect-schema.ts",
    "check:graphql": "npm run graphql:codegen && git diff --exit-code src/graphql/generated/sdk.ts && ! grep -rn 'fetch(' src/ --include='*.ts' --include='*.tsx' | grep -v 'node_modules' | grep -v 'scripts/' | grep -qi 'graphql'",
    "diagnose:backend": "tsx scripts/diagnose-backend.ts"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@tanstack/react-query": "^5.60.0",
    "@tanstack/react-router": "^1.82.1",
    "dexie": "^4.0.9",
    "dexie-react-hooks": "^1.1.7",
    "firebase": "^12.8.0",
    "graphql-request": "^7.4.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^6.1.1",
    "@graphql-codegen/typescript": "^5.0.7",
    "@graphql-codegen/typescript-graphql-request": "^6.4.0",
    "@graphql-codegen/typescript-operations": "^5.0.7",
    "@tanstack/router-devtools": "^1.82.1",
    "@tanstack/router-plugin": "^1.81.9",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.3.3",
    "dotenv": "^17.2.3",
    "graphql": "^16.12.0",
    "tsx": "^4.21.0",
    "typescript": "~5.6.2",
    "vite": "^5.4.10",
    "vite-plugin-pwa": "^0.21.0",
    "workbox-window": "^7.1.0"
  }
}
